<?php

namespace Drupal\comp_install\Event;

use Drupal\taxonomy\Entity\Term;

/**
* @file
* Contains comp_install.install.
*/

function comp_install_install() {
  // Create default taxonomny terms.
  // Gender.
  $genders = [
    'Not Identified',
    'Female',
    'Male',
  ];

  foreach ($genders as $gender) {
    $term = Term::create([
      'vid' => 'gender',
      'name' => $gender,
    ]);

    $term->save();
  }
}
